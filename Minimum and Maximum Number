#include<stdio.h>
#include<conio.h>

int main()
{
	int a,b,c,d,e;
	printf("Enter 5 numbers\n");
	scanf("%d %d %d %d %d",&a ,&b ,&c ,&d ,&e);
	
	if (a>b && a>c && a>d && a>e)
	{
	
		printf("%d is the maximum number among these\n",a);
	}
	else if (b>a && b>c && b>d && b>e)
	{
		printf("%d is the maximum number among these\n",b);
	}
	else if (c>a && c>b && c>d && c>e)
	{
		printf("%d is the maximum number among these\n",c);
	}
	else if (d>a && d>c && d>b && d>e)
	{
		printf("%d is the maximum number among these\n",d);
	}
	else
	{
		printf("%d is the maximum number among these\n",e);
	}
	
		
	if (a<b && a<c && a<d && a<e)
	{
	
		printf("%d is the minimum number among these",a);
	}
	else if (b<a && b<c && b<d && b<e)
	{
	
		printf("%d is the minimum number among these",b);
	}
	else if (c<a && c<b && c<d && c<e)
	{
	
		printf("%d is the minimum number among these",c);
	}
	else if (d<a && d<b && d<c && d<e)
	{
	
		printf("%d is the minimum number among these",d);
	}
	else 
	{
	
		printf("%d is the minimum number among these",e);
	}
}
